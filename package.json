{
  "name": "conventional-component",
  "version": "0.0.1",
  "description": "React components which can have their state hoisted into Redux.",
  "main": "dist/index.js",
  "typings": "./index.d.ts",
  "repository": "git@github.com:sebinsua/conventional-component.git",
  "author": "Seb Insua <me@sebinsua.com>",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "commitmsg": "commitlint -e",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "test": "jest --modulePathIgnorePatterns example/",
    "build": "babel src --out-dir dist --ignore __tests__/*.js",
    "release": "npm test && npm run build && standard-version"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0",
    "react-redux": "*",
    "redux": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "standard-version": "^4.2.0"
  }
}
